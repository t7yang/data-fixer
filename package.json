{
  "name": "data-fixer",
  "version": "1.1.3",
  "description": "a tool to check and fix data with schema, inspired by angular reactive form.",
  "author": "t7yang <t7yang@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:t7yang/data-fixer.git"
  },
  "keywords": [
    "data-fixer",
    "schema",
    "fix"
  ],
  "sideEffects": false,
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "update": "yarn upgradeInteractive",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:tsc": "tsc --noEmit",
    "build:esm": "tsc --build tsconfig.esm.json",
    "build:cjs": "tsc --build tsconfig.cjs.json",
    "build:all": "npm run build:esm && npm run build:cjs",
    "build": "npm run build:all"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/ramda": "^0.26.8",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4"
  }
}
