{
  "name": "data-fixer",
  "version": "2.0.0",
  "description": "a tool to check and fix data with schema, inspired by angular reactive form.",
  "author": "t7yang <t7yang@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:t7yang/data-fixer.git"
  },
  "keywords": [
    "data-fixer",
    "schema",
    "fix"
  ],
  "sideEffects": false,
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "update": "yarn upgradeInteractive",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:tsc": "tsc --noEmit",
    "clean": "rm -fr dist",
    "build:esm": "tsc --build tsconfig.esm.json",
    "build:cjs": "tsc --build tsconfig.cjs.json",
    "build:all": "npm run build:esm && npm run build:cjs",
    "build": "npm run clean && npm run build:all"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.4",
    "ajv": "^6.10.2",
    "ramda": "^0.26.1",
    "ts-toolbelt": "^6.1.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/ramda": "^0.26.39",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  }
}
